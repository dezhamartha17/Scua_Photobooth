{
  "name": "photo-booth-app",
  "version": "1.0.0",
  "description": "A photo booth application using Electron and Express.js",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "package-win": "electron-packager . PhotoboothV3 --platform=win32 --arch=x64 --out=dist --overwrite --icon=public/icon.ico",
    "build": "electron-builder"
  },
  "dependencies": {
    "@ar-js-org/ar.js": "^3.4.5",
    "aframe": "^1.6.0",
    "axios": "^1.7.4",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "config": "^3.3.12",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-fileupload": "^1.5.0",
    "googleapis": "^140.0.1",
    "kioskboard": "^2.3.0",
    "midtrans-client": "^1.3.1",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.14",
    "swiper": "^11.1.8"
  },
  "devDependencies": {
    "electron": "^25.2.0",
    "electron-builder": "^23.6.0",
    "electron-packager": "^17.1.2"
